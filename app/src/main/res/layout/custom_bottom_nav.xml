<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layoutDirection="ltr"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/white_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@drawable/back_top_bottom_nav"
        app:layout_constraintBottom_toTopOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7" />

    <View
        android:id="@+id/homes"
        android:layout_width="0dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cake" />

    <View
        android:id="@+id/cake"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/homes"
        app:layout_constraintEnd_toStartOf="@+id/homes"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/homes" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/cake"
        app:layout_constraintEnd_toStartOf="@+id/cake"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/pastry"
        app:layout_constraintTop_toTopOf="@+id/cake" />

    <View
        android:id="@+id/pastry"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <View
        android:id="@+id/profile"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/pastry"
        app:layout_constraintEnd_toStartOf="@+id/pastry"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/pastry" />

    <ImageView
        android:id="@+id/ic_home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@string/ic_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/homes"
        app:layout_constraintTop_toTopOf="@+id/homes"
        app:srcCompat="@drawable/ic_home" />

    <ImageView
        android:id="@+id/ic_cake"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@string/ic_cake"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cake"
        app:layout_constraintStart_toStartOf="@+id/cake"
        app:layout_constraintTop_toTopOf="@+id/cake"
        app:srcCompat="@drawable/ic_cake" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginBottom="28dp"
        android:contentDescription="@string/img_back"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toStartOf="@+id/cake"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:srcCompat="@drawable/ic_polygon" />

    <ImageView
        android:id="@+id/ic_shopping_bag"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:contentDescription="@string/ic_shopping_bag"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:srcCompat="@drawable/ic_shopping_bag" />

    <ImageView
        android:id="@+id/ic_pastry"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@string/ic_pastry"
        app:layout_constraintBottom_toBottomOf="@+id/pastry"
        app:layout_constraintEnd_toEndOf="@+id/pastry"
        app:layout_constraintStart_toStartOf="@+id/pastry"
        app:layout_constraintTop_toTopOf="@+id/pastry"
        app:srcCompat="@drawable/ic_pastry" />

    <ImageView
        android:id="@+id/ic_user"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@string/ic_user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profile"
        app:srcCompat="@drawable/ic_user" />


    <androidx.constraintlayout.widget.Group
        android:id="@+id/off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:constraint_referenced_ids="view6, txtShoppingCount" />

    <View
        android:id="@+id/view6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/back_shopping_bag_cart"
        app:layout_constraintBottom_toTopOf="@+id/ic_shopping_bag"
        app:layout_constraintEnd_toEndOf="@+id/ic_shopping_bag"
        app:layout_constraintStart_toEndOf="@+id/ic_shopping_bag"
        app:layout_constraintTop_toTopOf="@+id/ic_shopping_bag"/>


    <TextView
        android:id="@+id/txtShoppingCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="55"
        android:textSize="8sp"
        android:textColor="@color/white_main"
        app:layout_constraintBottom_toBottomOf="@+id/view6"
        app:layout_constraintEnd_toEndOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6"
        tools:ignore="SmallSp" />

    <View
        android:id="@+id/indicator_home"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/back_selected_nav_bottom"
        app:layout_constraintBottom_toTopOf="@+id/homes"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintHorizontal_bias="0.596"
        app:layout_constraintStart_toStartOf="@+id/homes"
        app:layout_constraintTop_toTopOf="@+id/homes" />

    <View
        android:id="@+id/indicator_cake"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:visibility="gone"
        android:background="@drawable/back_selected_nav_bottom"
        app:layout_constraintBottom_toTopOf="@+id/cake"
        app:layout_constraintEnd_toEndOf="@+id/cake"
        app:layout_constraintStart_toStartOf="@+id/cake"
        app:layout_constraintTop_toTopOf="@+id/cake" />


    <View
        android:id="@+id/indicator_pastry"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:visibility="gone"
        android:background="@drawable/back_selected_nav_bottom"
        app:layout_constraintBottom_toTopOf="@+id/pastry"
        app:layout_constraintEnd_toEndOf="@+id/pastry"
        app:layout_constraintStart_toStartOf="@+id/pastry"
        app:layout_constraintTop_toTopOf="@+id/pastry" />


    <View
        android:id="@+id/indicator_user"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:layout_marginStart="4dp"
        android:visibility="gone"
        android:background="@drawable/back_selected_nav_bottom"
        app:layout_constraintBottom_toTopOf="@+id/profile"
        app:layout_constraintEnd_toEndOf="@+id/profile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/profile"
        app:layout_constraintTop_toTopOf="@+id/profile" />


</androidx.constraintlayout.widget.ConstraintLayout>